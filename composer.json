{
    "name": "acdh-oeaw/arche-lib-ingest",
    "description": "A library for ingesting data into the ACDH digital objects repository",
    "keywords": [],
    "homepage": "https://github.com/acdh-oeaw/arche-lib-ingest",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateusz Żółtak",
            "email": "mzoltak@oeaw.ac.at"
        }
    ],
    "require": {
        "php": ">=8 <8.3",
        "ext-iconv": "*",
        "guzzlehttp/guzzle": "^7",
        "acdh-oeaw/arche-lib": "dev-rdfInterface",
        "acdh-oeaw/uri-normalizer": "dev-master",
        "zozlak/rdf-constants": "^1.2.1",
        "kbsali/redmine-api": "^2.2",
        "sweetrdf/rdf-interface": "^2.0.0-RC1",
        "sweetrdf/quick-rdf": "dev-master",
        "sweetrdf/quick-rdf-io": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "acdhOeaw\\": "src/acdhOeaw"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "acdhOeaw\\arche\\lib\\ingest\\tests\\": "tests"
        }
    }
}
